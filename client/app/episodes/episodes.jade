tags-input(ng-model='item.season' placeholder='Search season' add-from-autocomplete-only='true' display-property='title' key-property='_id' max-tags='1')
  auto-complete(source='loadSeasons($query)' template='season-template' min-length='0'
  load-on-focus='true'
  load-on-empty='true'
  max-results-to-show='32')
script(type='text/ng-template' id='season-template')
  .tag-template
    span {{data.title}}
